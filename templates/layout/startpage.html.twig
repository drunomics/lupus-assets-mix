{#
/**
* @file
* Theme to display the start page via page manager.
*
* Regions:
*   - content.header: Header
*   - content.content: Content
*   - content.teaser: Teaser
*/
#}
{% embed '@layouts/startpage/startpage.twig' %}

    {% block header %}
      {{ content.header }}
    {% endblock %}

    {% block content %}
      {{ content.content }}
    {% endblock %}

    {% block teaser %}
      {{ content.teaser }}
    {% endblock %}

{%  endembed %}
